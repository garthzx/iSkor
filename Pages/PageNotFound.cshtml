@page
@model EventScoringWeb.Pages.PageNotFoundModel
@{
    Layout = "_NoDashboardLayout";
}

<div class="page-error-container">
    <div class="page-error-container2 row justify-content-center">
        <div class="col-12 col-md-6">
            <div class="m-2 mt-5 text-center">
                <h1 class="p-5 page-error-title">404</h1>
                <h2 class="page-error-heading text-uppercase">Page Not Found!</h2>
                <p class="page-error-p ">
                    The page you are looking for might have been removed or had its name changed or it is temporary unavailable
                </p>
            </div>
        </div>
    </div>
</div>

